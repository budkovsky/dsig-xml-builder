{
	"name": "budkovsky/dsig-xml-builder",
	"description": "XML Digital Signature builder for PHP7.1+",
	"autoload": {
		"psr-4": {
			"Budkovsky\\DsigXmlBuilder\\": "src"
		}
	},
	"autoload-dev": {
	   "psr-4": {
	       "Budkovsky\\DsigXmlBuilder\\Tests\\": "tests"
	   }
	},
	"require": {
		"php": "^7.1",
		"ext-openssl": "*",
		"budkovsky/aid": "^0.1.14",
		"budkovsky/openssl-wrapper": "^0.1.1"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"ext-tokenizer": "*",
		"phpunit/phpunit": "^7.0",
		"squizlabs/php_codesniffer": "^3.0",
		"sebastian/phpcpd": "^4.0",
		"theseer/phpdox": "^0.12.0",
		"phploc/phploc": "^4.0",
		"pdepend/pdepend": "^2.7",
		"phpmd/phpmd": "^2.8"
	},
	"repositories": [
		{
			"type": "git",
			"url": "https://Budkovsky@github.com/budkovsky/extended-dom-element",
			"name": "budkovsky/extended-dom-element"
		},
		{
			"type": "git",
			"url": "https://github.com/budkovsky/aid",
			"name": "budkovsky/aid"
		},
		{
		  "type": "git",
		  "url": "https://github.com/budkovsky/openssl-wrapper",
		  "name": "budkovsky/openssl-wrapper"
		}
	],
	"license": "MIT",
	"scripts": {
	   "test": "vendor/bin/phpunit --bootstrap vendor/autoload.php tests"
   }
}
